{
  "name": "DOM manipulation Example",
  "version": "1.0",
  "description": "Chrome Extension Example for manipulating DOM.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Dragg'z",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "tabs",
    "http://192.168.1.194:8000/"
  ],
  "content_scripts": [    
       {
           "matches": [
               "http://*/*",
               "https://*/*"
           ],
           "js": ["interact.js",
                  "jquery.min.js",
                  "content.js"],
           "all_frames": true,
           "run_at": "document_idle"
       }
   ],
   "web_accessible_resources": ["jquery.min.map"],
  "manifest_version": 2
}